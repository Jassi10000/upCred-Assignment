<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title></title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="">
    </head>
    <body>
      
        <h1>Edit your details in the contact book</h1>
        

        <% userData.forEach(function(element) { %>
            <form action="/update/<%= element._id %>" method="POST">
                <input type="text" name="userName" value = "<%= element.userName %>">
                <input type="email" name="userEmail" value = "<%= element.userEmail %>">
                <input type="number" name="userPhoneNo" value = "<%= element.userPhoneNo %>">
                <br><br>
            <% }) %>
            <button type="submit">Submit</button>

            </form>
            
        <script src="" async defer></script>
    </body>
</html>